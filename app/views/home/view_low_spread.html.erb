<h1>Negative Spread Cards</h1>
<table border="1">
	<tr>
		<th>Card</th>

		<th>TCG Mid</th>
		<th>Card Kingdom</th>
		<th>Spread</th>
		
	</tr>
<% 

		negativeCards.each do |x| 
			foilTxt = ""
			if x['isFoil'] 
				foilTxt = " - FOIL "
			end

%>
		<tr>
		
				<td><%=x['name'] + foilTxt + ' ('+ x['set'] + ')' %> </td>
				<td><%=x["tcgPrice"]%></td>
				
				<td><%=x["ckPrice"]%></td>
				
				<td><%= x["spread"] %></td>
		
		</tr>
		<%
	end
		

		%>

</table>